    "cargo": {
      "type": "object",
      "description": "With cargo you have two options, detailed or simple. These are both expressed directly on the cargo object. For detailed cargo, `shipping_units` is required, and `weight` and `volume` is required in the `cargo` object or the `shipping_units` object. For simple cargo, `shipping_units` is required, but only one should be specified, and `weight` and `volume` are required in the `cargo` object.",
      "properties": {
        "_object": {
          "type": "string",
          "description": "String representing the object’s type. Always `/cargo` for this object."
        },
        "volume": {
          "type": "object",
          "properties": {
            "_object": {
              "type": "string",
              "description": "Type of the object"
            },
            "value": {
              "type": "number",
              "description": "total volume"
            },
            "unit": {
              "type": "string",
              "description": "unit of measurement. \"cbm\" for cubic meters. \"cbft\" for cubic feet.",
              "enum": [
                "cbm",
                "cbft"
              ]
            }
          }
        },
        "weight": {
          "type": "object",
          "properties": {
            "_object": {
              "type": "string",
              "description": "String representing the object’s type. Always `/quantity/weight` for this object."
            },
            "value": {
              "type": "number",
              "description": "Required. Specifies the weight as a float."
            },
            "unit": {
              "type": "string",
              "description": "Required. Unit of measurement. \"kg\" for kilograms and \"lbs\" for pounds.",
              "enum": [
                "kg",
                "lbs"
              ]
            }
          }
        },
        "marks_and_numbers": {
          "type": "string",
          "description": "Optional. Identifying markings and numbers that are visible on the cargo.",
          "default": "Fragile"
